{
  "name": "@toolbarthomas/harbor",
  "version": "0.908.0",
  "type": "module",
  "description": "Boilerplate for Drupal Themes",
  "exports": "./Harbor/index.js",
  "bin": {
    "harbor": "./index.js"
  },
  "node": ">=18.18.0",
  "scripts": {
    "development": "node ./index.js --watch --styleguide --verbose",
    "export": "node ./index.js --task=export",
    "images": "node ./index.js --task=images",
    "javascripts": "node ./index.js --task=javascripts",
    "predevelopment": "npm run production",
    "production": "node ./index.js --task=prepare,compile --minify",
    "postproduction": "node ./index.js --task=export,setup",
    "resolve": "node ./index.js --task=resolve",
    "setup": "node ./index.js --task=setup",
    "styleguide": "node ./index.js --styleguide",
    "stylesheets": "node ./index.js --task=stylesheets",
    "test:approve": "node ./index.js --task=test --test=approve",
    "test:reference": "node ./index.js --task=test --test=reference",
    "test": "node ./index.js --task=test"
  },
  "files": [
    "./index.js",
    ".babelrc",
    ".browserlistrc",
    ".env",
    ".eslintignore",
    ".eslintrc",
    ".prettierrc",
    ".storybook/index.ejs",
    ".stylelintrc",
    "harbor.default.config.js",
    "Harbor/",
    "LICENCE",
    "README.md",
    "TIPS.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toolbarthomas/harbor.git"
  },
  "keywords": [
    "Drupal",
    "Theme"
  ],
  "author": "Thomas van der Velde <contact@toolbarthomas.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toolbarthomas/harbor/issues"
  },
  "homepage": "https://github.com/toolbarthomas/harbor#readme",
  "dependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.7",
    "@babel/eslint-parser": "^7.25.7",
    "@babel/plugin-proposal-decorators": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/builder-webpack5": "^8.3.5",
    "@storybook/cli": "^8.3.5",
    "@storybook/html-webpack5": "^8.3.5",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "camelcase": "^8.0.0",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.1",
    "concat": "^1.0.3",
    "copyfiles": "^2.4.1",
    "cssnano": "^7.0.6",
    "dotenv": "16.4.5",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-prettier": "^5.2.1",
    "glob": "^11.0.0",
    "html-escaper": "^3.0.3",
    "imagemin": "^9.0.0",
    "is-svg": "^5.1.0",
    "log-symbols": "^7.0.0",
    "mkdirp": "^3.0.1",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "node-sass-glob-importer": "^5.3.3",
    "outdent": "^0.8.0",
    "postcss": "^8.4.47",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "sass": "^1.79.4",
    "snake-case": "^4.0.0",
    "stringify-object": "^5.0.0",
    "stylelint": "^16.9.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-scss": "^6.7.0",
    "svgo": "^3.3.2",
    "svgstore": "^3.0.1",
    "twing": "^5.2.2",
    "twing-loader": "^4.0.1",
    "type-name": "^2.0.2",
    "uglify-js": "^3.19.3",
    "webpack": "^5.95.0",
    "ws": "^8.18.0",
    "yaml-loader": "^0.8.1"
  },
  "devDependencies": {
    "@storybook/preview-api": "^8.3.5"
  }
}
